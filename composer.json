{
    "name": "dazzle-php/redis",
    "description": "Dazzle Asynchronous Redis Driver.",
    "keywords": [
        "dazzle", "dazzle-php", "async", "async-redis", "redis", "redis-client", "cache", "queue", "client"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/dazzle-php/redis/issues",
        "source": "https://github.com/dazzle-php/redis"
    },
    "authors": [
        {
            "name":     "Kamil Jamroz",
            "homepage": "https://github.com/khelle"
        },
        {
            "name":     "Hidehalo",
            "homepage": "https://github.com/hidehalo"
        },
        {
            "name":     "The contributors",
            "homepage": "http://github.com/dazzle-php/channel/contributors"
        }
    ],
    "require": {
        "php": ">=5.6.7",
        "dazzle-php/event": "0.5.*",
        "dazzle-php/loop": "0.5.*",
        "dazzle-php/promise": "0.5.*",
        "dazzle-php/socket": "0.5.*",
        "dazzle-php/throwable": "0.5.*",
        "dazzle-php/util": "0.5.*",
        "clue/redis-protocol": "0.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8.0 <5.4.0"
    },
    "autoload": {
        "psr-4": {
            "Dazzle\\Redis\\": "src/Redis",
            "Dazzle\\Redis\\Test\\": "test"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.5-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
